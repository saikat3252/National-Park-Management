<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="476.0" prefWidth="769.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.simulating_operations_of_a_national_park_management.Local_Community_Member.CommunityProjectDashBoradController">

    <Label layoutX="20" layoutY="20" text="Community Projects" />

    <Label layoutX="20" layoutY="60" text="Monitor community project progress" />

    <TableView layoutX="20" layoutY="100" prefHeight="300" prefWidth="400">
        <columns>
            <TableColumn fx:id="projectableColumn" prefWidth="142.0" text="Project" />
            <TableColumn fx:id="progressTableColumn" prefWidth="135.0" text="Progress" />
            <TableColumn fx:id="StatusTableColumn" prefWidth="122.0" text="Status" />
        </columns>
    </TableView>

    <Label layoutX="450" layoutY="100" text="Project Details" />

    <TextArea fx:id="ProjectDetailsTextArea" layoutX="450" layoutY="130" prefHeight="150" prefWidth="300" />

    <Label layoutX="450" layoutY="290" text="Selected Project" />
    <Label layoutX="550" layoutY="290" text="None" />

    <Button layoutX="450" layoutY="320" onAction="#ViewPrjDetailsOnActionButton" prefWidth="100" text="View Details" />
    <Button layoutX="560" layoutY="320" onAction="#UpdateProgressButton" prefWidth="120" text="Update Progress" />

    <Button layoutX="20" layoutY="420" onAction="#BackOnActionButton" prefWidth="80" text="Back" />

</AnchorPane>
